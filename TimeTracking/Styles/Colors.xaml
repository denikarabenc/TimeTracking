<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--Window-->
    <Color x:Key="WindowStartGradient">#1D142A</Color>
    <Color x:Key="WindowEndGradient">#100B18</Color>
    
    <!--Button-->
    <SolidColorBrush x:Key="Foreground"
    Color="#FFFFFFFF" />
    
    <Color x:Key="ButtonStartGradient">#7D2EF4</Color>
    <Color x:Key="ButtonEndGradient">#510ABC</Color>

    <Color x:Key="ButtonMouseOverStartGradient">#9E64F6</Color>
    <Color x:Key="ButtonMouseOverEndGradient">#6932BB</Color>

    <Color x:Key="ButtonPressedStartGradient">#5908D4</Color>
    <Color x:Key="ButtonPressedEndGradient">#370287</Color>
    
    <!--DataGrid-->
    <SolidColorBrush x:Key="DataGridRowBordersColor"
    Color="#1B0D3C" />
    <SolidColorBrush x:Key="DataGridSelectRowColor"
    Color="#321C78" />
    <SolidColorBrush x:Key="DataGridSelectRowBordersColor"
    Color="#554c74" />
</ResourceDictionary>